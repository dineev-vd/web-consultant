{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "es6",
      "dom"
    ],
    "outDir": "www",
    "rootDirs": [
      "src",
      "../common"
    ],
    "target": "ES6",
    "paths": {
      "@common/*": [
        "../common/*"
      ]
    },
    "declaration": false,
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
  }
}